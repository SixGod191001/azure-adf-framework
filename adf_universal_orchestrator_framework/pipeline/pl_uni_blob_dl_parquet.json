						"parameters": {
							"p_container": {
								"value": "@pipeline().parameters.source_container",
								"type": "Expression"
							},
							"p_filename": {
								"value": "@pipeline().parameters.source_filename",
								"type": "Expression"
							},
							"p_sheet": {
