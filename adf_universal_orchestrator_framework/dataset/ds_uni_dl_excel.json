		},
		"parameters": {
			"p_container": {
				"type": "string",
				"defaultValue": "landing"
			},
			"p_path": {
				"type": "string",
				"defaultValue": "cpa"
			},
			"p_filename": {
				"type": "string",
				"defaultValue": "CPA_Franchise_Marketing_Mapping.xlsx"
			},
			"p_sheet": {
				"type": "string",
				"defaultValue": "Sheet1"
			}
		},
