{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFUniversalOrchestratorFramework"
		},
		"lk_blob_cedc_new_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lk_blob_cedc_new'"
		},
		"lk_dl_cedc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lk_dl_cedc'"
		},
		"lk_dl_cedc_2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lk_dl_cedc_2'"
		},
		"AKVLinkedService_UniversalOrchestratorFramework_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adf-framework.vault.azure.net/"
		},
		"SupportDatabase_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "adf-framework-metadata-db-connection-string"
		},
		"lk_dl_cedc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cedcdatalake.dfs.core.windows.net/"
		},
		"lk_dl_cedc_2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cedcdatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFUniversalOrchestratorFramework_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lk_blob_cedc_new_connectionString": {
						"value": "[parameters('lk_blob_cedc_new_connectionString')]"
					},
					"lk_dl_cedc_accountKey": {
						"value": "[parameters('lk_dl_cedc_accountKey')]"
					},
					"lk_dl_cedc_2_accountKey": {
						"value": "[parameters('lk_dl_cedc_2_accountKey')]"
					},
					"AKVLinkedService_UniversalOrchestratorFramework_properties_typeProperties_baseUrl": {
						"value": "[parameters('AKVLinkedService_UniversalOrchestratorFramework_properties_typeProperties_baseUrl')]"
					},
					"lk_dl_cedc_properties_typeProperties_url": {
						"value": "[parameters('lk_dl_cedc_properties_typeProperties_url')]"
					},
					"lk_dl_cedc_2_properties_typeProperties_url": {
						"value": "[parameters('lk_dl_cedc_2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFUniversalOrchestratorFramework_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SupportDatabase_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('SupportDatabase_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFUniversalOrchestratorFramework_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADFUniversalOrchestratorFramework_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFUniversalOrchestratorFramework_ArmTemplate_1')]"
			]
		},
		{
			"name": "ADFUniversalOrchestratorFramework_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFUniversalOrchestratorFramework_ArmTemplate_2')]"
			]
		}
	]
}